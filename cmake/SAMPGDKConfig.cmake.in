set(SAMPGDK_VERSION @SAMPGDK_VERSION@)

@PACKAGE_INIT@

set_and_check(SAMPGDK_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(SAMPGDK_LIBRARY_DIR "@PACKAGE_LIBRARY_INSTALL_DIR@")

set(SAMPGDK_INCLUDE_DIRS ${SAMPGDK_INCLUDE_DIR}
    CACHE PATH "SA-MP GDK include directories")
set(SAMPGDK_LIBRARY_DIRS ${SAMPGDK_LIBRARY_DIR}
    CACHE PATH "SA-MP GDK library directories")
